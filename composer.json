{
    "name": "alsbury/chiphpotle-rest",
    "description": "Library to interact SpiceDB REST API",
    "keywords": [
        "rest",
        "spicedb",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "David Alsbury",
            "email": "david@alsbury.com"
        },
        {
            "name": "Michael O'Connell",
            "email": "wundbread@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "jane-php/open-api-runtime": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "jane-php/open-api-2": "^7.3",
        "vlucas/phpdotenv": "^5.5",
        "friendsofphp/php-cs-fixer": "^3.34",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Chiphpotle\\Rest\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chiphpotle\\Rest\\Test\\" : "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
